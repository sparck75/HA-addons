{
    "name": "AppDaemonDev",
    "version": "latest",
    "slug": "appdaemondev",
    "description": "AppDaemon version (latest) for Home Assistant. Build using master branch.",
    "url": "https://github.com/sparck75/hassio-addons/appdaemon_dev",
    "startup": "after",
    "image": "sparck75/{arch}-addon-appdaemon-dev",
    "boot": "auto",
    "map": ["config:rw","addons:rw","ssl"],
    "options": {
        "domain": "x.x.x.x",
        "commtype": "SSE",
        "debugging": "none"
    },
    "schema": {
        "domain": "str",
        "commtype": "str",
        "debugging": "str"
    }, 
    "ports": {
        "80/tcp": 5050
    }
}
